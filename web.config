<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="node" path="*" verb="*" modules="iisnode" scriptProcessor="C:\Program Files (x86)\iisnode\iisnode.dll" resourceType="Unspecified" />
    </handlers>
    <rewrite>
      <rules>
        <rule name="Nodejs">
          <match url="(.*)" />
          <action type="Rewrite" url="server.js" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
